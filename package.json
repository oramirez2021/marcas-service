{
    "name": "buscar-manifiesto-service",
    "version": "1.0.0",
    "private": true,
    "main": "dist/main.js",
    "scripts": {
      "build": "nest build",
      "build:layer": "./scripts/build-layer.sh",
      "build:lambda": "npm run build && npm run build:layer && sam build",
      "build:docker": "npm run build && npm ci --only=production",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "./start-with-oracle.sh",
      "debug": "node --inspect-brk -r ts-node/register node_modules/@nestjs/cli/bin/nest.js start --watch",
      "clean": "rm -rf dist layers/dependencies/nodejs/node_modules",
      "sam:build": "npm run build:lambda",
      "sam:deploy": "sam deploy --guided",
      "sam:deploy:prod": "sam deploy --parameter-overrides file://parameters.json",
      "sam:local": "sam local start-api --port 3000",
      "test:token": "node scripts/generate-test-token.js",
      "test:sqs": "node scripts/test-sqs-message.js",
      "test:sqs:typed": "node scripts/test-typed-sqs-message.js",
      "docker:build": "docker build -t buscar-manifiesto-service:latest .",
      "docker:run": "docker run -p 3000:3000 --env-file .env buscar-manifiesto-service:latest",
      "docker:k8s": "docker-compose -f docker-compose.k8s.yml up --build",
      "docker:k8s:down": "docker-compose -f docker-compose.k8s.yml down",
      "docker:test": "npm run docker:build && npm run docker:run"
    },
    "dependencies": {
      "@fastify/aws-lambda": "^6.1.1",
      "@nestjs/common": "^10.4.20",
      "@nestjs/config": "^3.2.3",
      "@nestjs/core": "^10.4.20",
      "@nestjs/platform-express": "^10.4.20",
      "@nestjs/platform-fastify": "^10.4.20",
      "@nestjs/swagger": "^7.4.2",
      "@nestjs/typeorm": "^10.0.2",
      "@types/compression": "^1.8.1",
      "@types/helmet": "^0.0.48",
      "@types/joi": "^17.2.2",
      "@types/morgan": "^1.9.10",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.2",
      "compression": "^1.8.1",
      "helmet": "^8.1.0",
      "joi": "^18.0.1",
      "jsonwebtoken": "^9.0.2",
      "jwks-rsa": "^3.2.0",
      "morgan": "^1.10.1",
      "oracledb": "^6.10.0",
      "pg": "^8.11.3",
      "puppeteer": "^24.26.1",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.8.2",
      "serverless-http": "^3.2.0",
      "sqlite3": "^5.1.7",
      "swagger-ui-express": "^5.0.1",
      "typeorm": "^0.3.20",
      "xml2js": "^0.6.2"
    },
    "devDependencies": {
      "@nestjs/cli": "^7.6.0",
      "@nestjs/mapped-types": "^2.1.0",
      "@types/aws-lambda": "^8.10.156",
      "@types/xml2js": "^0.4.14",
      "typescript": "^5.2.2"
    }
  }
  